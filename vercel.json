{
  "functions": {
    "backend/app/main.py": {
      "runtime": "python3.9"
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "backend/app/main.py"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "env": {
    "VITE_API_URL": "https://spy-tracker-gamma.vercel.app"
  },
  "build": {
    "env": {
      "VITE_API_URL": "https://spy-tracker-gamma.vercel.app"
    }
  }
}